<cleaner id="vim" os="linux">
  <label>VIM</label>
  <description>Editor</description>
  <option id="history">
    <label>Historial</label>
    <description>Eliminar ~/.viminfo que incluye historial de archivos, historial de comandos y búferes</description>
    <action command="delete" search="file" path="~/.viminfo"/>
  </option>
</cleaner>
