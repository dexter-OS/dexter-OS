<!-- verificado con SQLite versión 3.7.13 en Ubuntu 12.10 -->
<!-- verificado con SQLite versión 3.11.0 en Ubuntu 16.04 -->
<cleaner id="sqlite3" os="linux">
  <label>sqlite3</label>
  <option id="history">
    <label>Historial</label>
    <description>Eliminar el historial de comandos</description>
    <action command="delete" search="file" path="~/.sqlite_history"/>
  </option>
</cleaner>
