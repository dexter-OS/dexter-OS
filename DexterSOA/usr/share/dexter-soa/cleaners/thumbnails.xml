<cleaner id="thumbnails" os="linux">
  <label translate="true">Miniaturas</label>
  <description>Iconos para archivos en el sistema</description>
  <option id="cache">
    <label>Caché</label>
    <description>Eliminar la caché</description>
    <action command="delete" search="walk.files" path="~/.thumbnails/"/>
    <action command="delete" search="walk.files" path="$XDG_CACHE_HOME/thumbnails/"/>
  </option>
</cleaner>
